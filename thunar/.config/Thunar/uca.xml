<?xml encoding="UTF-8" version="1.0"?>
<actions>
<action>
	<icon>utilities-terminal</icon>
	<name>Open Terminal Here</name>
	<unique-id>1475400350665553-1</unique-id>
	<command>exo-open --working-directory %f --launch TerminalEmulator</command>
	<description>Example for a custom action</description>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
</action>
<action>
	<icon>folder-publicshare-symbolic</icon>
	<name>Start HTTP File Server</name>
	<unique-id>1446987438782448-8</unique-id>
	<command>xfce4-terminal -e &apos;sh -c &quot;ip addr; webfsd -Fdp 8000&quot;&apos;</command>
	<description></description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>7zip</icon>
	<name>Create zip</name>
	<unique-id>1437000933183166-1</unique-id>
	<command>zip -r %n.zip %N</command>
	<description></description>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>furiusisomount</icon>
	<name>Mount Image</name>
	<unique-id>1446736398542705-2</unique-id>
	<command>udisksctl loop-setup -r -f %f</command>
	<description>mount image files using udiskctl</description>
	<patterns>*.iso;*.ISO;*.img;*.IMG</patterns>
	<other-files/>
</action>
<action>
	<icon>atom</icon>
	<name>Open in Atom</name>
	<unique-id>1447677815056315-1</unique-id>
	<command>atom %f</command>
	<description>Open this Folder with Atom Editor</description>
	<patterns>*</patterns>
	<directories/>
</action>
</actions>
